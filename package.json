{
  "name": "eFins",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "devserver": "node_modules/pm2/bin/pm2 start ./app-dev.json --log-date-format \"YYYY-MM-DD HH:mm Z\"",
    "start": "node_modules/pm2/bin/pm2 start bin/eFins -i 4 --log-date-format \"YYYY-MM-DD HH:mm Z\"",
    "stop": "node_modules/pm2/bin/pm2 stop -s eFins; node_modules/pm2/bin/pm2 stop -s eFins-dev; node_modules/pm2/bin/pm2 stop -s test-eFins; echo \"Stopped\";",
    "reload": "node_modules/pm2/bin/pm2 gracefulReload bin/eFins",
    "monitor": "node_modules/pm2/bin/pm2 monitor",
    "logs": "node_modules/pm2/bin/pm2 logs",
    "test": "bin/test"
  },
  "dependencies": {
    "bcryptjs": "^2.1.0",
    "bluebird": "^2.9.14",
    "body-parser": "~1.8.1",
    "cheerio": "^0.18.0",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "epilogue": "^0.5.0",
    "express": "~4.9.0",
    "express-bearer-token": "^2.1.0",
    "express-better-ratelimit": "^1.1.2",
    "grunt": "^0.4.5",
    "hjs": "~0.0.6",
    "istanbul": "^0.3.6",
    "istanbul-middleware": "^0.2.0",
    "jsonwebtoken": "^3.2.2",
    "longjohn": "^0.2.4",
    "morgan": "~1.3.0",
    "nodeunit": "^0.9.0",
    "passport": "^0.2.1",
    "passport-http-bearer": "^1.0.1",
    "pg": "^4.2.0",
    "pg-hstore": "^2.3.1",
    "pm2": "^0.12.4",
    "request": "^2.53.0",
    "sendgrid": "^1.6.0",
    "sendgrid-stub": "git://github.com/davidbenhaim/sendgrid-stub",
    "sequelize": "2.0.5",
    "sequelize-cli": "^1.1.0",
    "serve-favicon": "~2.1.3",
    "url-join": "0.0.1"
  }
}
